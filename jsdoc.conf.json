{
  "plugins": ["plugins/markdown"],
  "templates": {
    "logoFile": "",
    "cleverLinks": true,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": false,
    "outputSourcePath": false,
    "systemName": "snoowrap",
    "footer": "<script src=\"snoowrap.js\"></script>",
    "copyright": "",
    "navType": "vertical",
    "theme": "yeti",
    "linenums": false,
    "collapseSymbols": false,
    "inverseNav": false,
    "protocol": "html://",
    "methodHeadingReturns": false,
    "includeDate": false,
    "syntaxTheme": "dark"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": false
  },
  "opts": {
    "sort": false,
    "destination": "./doc/",
    "template": "./node_modules/ink-docstrap/template",
    "readme": "./README.md"
  }
}
