{
  "name": "snoowrap",
  "version": "0.5.0",
  "license": "MIT",
  "description": "A Node.js wrapper for the reddit API",
  "main": "lib/snoowrap.js",
  "scripts": {
    "compile": "babel -d lib/ src/ && documentation build src/snoowrap.js --shallow -o doc/ -f html",
    "pretest": "eslint . && npm run compile",
    "test": "mocha --harmony_proxies --compilers js:babel-core/register",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/not-an-aardvark/snoowrap.git"
  },
  "keywords": [
    "reddit",
    "api",
    "wrapper",
    "praw",
    "snoo"
  ],
  "author": "not-an-aardvark <not-an-aardvark@users.noreply.github.com>",
  "bugs": {
    "url": "https://github.com/not-an-aardvark/snoowrap/issues"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-destructuring",
      "transform-es2015-parameters"
    ]
  },
  "homepage": "https://github.com/not-an-aardvark/snoowrap",
  "dependencies": {
    "bluebird": "^3.3.1",
    "harmony-reflect": "^1.4.2",
    "lodash": "^4.5.0",
    "moment": "^2.11.2",
    "promise-chains": "^0.2.2",
    "request-promise": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-plugin-transform-es2015-destructuring": "^6.5.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "chai": "^3.5.0",
    "documentation": "^4.0.0-beta",
    "mocha": "^2.4.5"
  }
}
